------------------------------------PHOTO------------------------------------------------------------
     function AOBRep(search, change)
	local aob = AOBScan(search)	if aob then	  for i=0,aob.Count-1 do  autoAssemble(aob[i]..':\ndb '..change)
	  end	  aob.Destroy()	end	end
	----------------------------------------------------
	all_addressfloat = {}
     function findFloatValueAndReplace(findValue , change)
    memscan = createMemScan()
    foundlist = createFoundList(memscan)
    protectionflags = "*W*X*C"
    memscan.firstScan(soExactValue, vtSingle, rtRounded, findValue, nil,
    "0","7fffffffffff",protectionflags,
     fsmAligned,"4",     false, false, false, false)
    memscan.waitTillDone()    foundlist.initialize()    for i=0, foundlist.Count-1 do
    all_addressfloat[i+1] = foundlist.Address[i]
    end
    foundlist.destroy()    memscan.destroy()    for i=1, #all_addressfloat do       writeFloat(all_addressfloat[i], change)
    end
  end
-------------------------------------------------------------------------------------------------------
	  all_address = {}
    function findDwordValueAndReplace(findValue , change)
    memscan = createMemScan()    foundlist = createFoundList(memscan)    protectionflags = "*W*X*C"
    memscan.firstScan(soExactValue, vtDword, rtRounded, findValue, nil,
    "0","7fffffffffff",protectionflags,           fsmAligned,"4",           false, false, false, false)
    memscan.waitTillDone()    foundlist.initialize()
    for i=0, foundlist.Count-1 do
    all_address[i+1] = foundlist.Address[i]
    end
    foundlist.destroy()    memscan.destroy()    for i=1, #all_address do
       writeInteger(all_address[i], change)
    end
  end
-----------------------------------------CHEAT TABEL----------------------------------------------------------------------------------------------
    function RedLineON()
	searchV = '00 00 80 40 00 00 80 3F 00 00 80 3F 00 00 00 00'
	replaceV = '00 00 48 42 00 00 80 3F 00 00 96 43 00 00 00 00'
	AOBRep(searchV,replaceV)	showMessage("âšœï¸âšœï¸âšœï¸Red Line Activadâšœï¸âšœï¸âšœï¸")	end
	function RedLineOFF()
	searchV = '00 00 48 42 00 00 80 3F 00 00 96 43 00 00 00 00'
	replaceV = '00 00 80 40 00 00 80 3F 00 00 80 3F 00 00 00 00'
	AOBRep(searchV,replaceV)	showMessage("âšœï¸âšœï¸âšœï¸Red Line Disabledâšœï¸âšœï¸âšœï¸")	end
--------------------------------------------------------
    function RedLightON()
	searchV = '00 00 00 00 00 00 80 40 00 00 80 3F 00 00 80 3F'
